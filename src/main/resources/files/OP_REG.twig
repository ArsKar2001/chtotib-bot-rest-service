<?xml version="1.0" encoding="utf-8"?>
<Файл ИдФайл="{{ IdFile }}.xml" ВерсФорм="{{ VersForm }}" КодСФРД="{{ CodeSFRD }}" КодНО="{{ CodeNO }}"{% if PrVisBum is defined %} ПрВысБум="{{ PrVisBum }}"{% endif %}>
    {% if RegType == 'ИП' %}
        <СвИП ИННФЛ="{{ INNFl }}" E-mail="{{ Email }}">
            <ФИО Фамилия="{{ PersonLastName }}" Имя="{{ PersonName }}"{% if PersonMiddleName != ''%} Отчество="{{ PersonMiddleName }}"{% endif %}></ФИО>
        </СвИП>
    {% else %}
        <СвЮЛ НаимЮЛПолн="{{ CompanyName }}"{% if CompanyOGRN %} ОГРН="{{ CompanyOGRN }}"{% endif %}></СвЮЛ><СвЗаявит ИННФЛ="{{ INNFl }}" E-mail="{{ Email }}"><ФИО Фамилия="{{ PersonLastName }}" Имя="{{ PersonName }}"{% if PersonMiddleName != ''%} Отчество="{{ PersonMiddleName }}"{% endif %}></ФИО></СвЗаявит>
    {% endif %}
    {% for file in files %}
        {% if file.CodeSVDREG %}
            <Документ ИдДок="{{ file.IdDoc }}" КодСВДРЕГ="{{ file.CodeSVDREG }}" типСодержимого="{{ file.ContainType }}" ИсхИмяФайла="{{ file.filename }}" ИмяФайла="{{ file.filename }}">
                <Подпись ИмяФайла="{{ file.filename }}.p7s" Роль="МФЦ">
                    <ФИО Фамилия="{{ file.signer.lastname }}" Имя="{{ file.signer.firstname }}" Отчество="{{ file.signer.patronymic }}"></ФИО>
                </Подпись>
            </Документ>
        {% endif %}
    {% endfor %}
</Файл>